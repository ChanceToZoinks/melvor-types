{
  "compilerOptions": {
    "checkJs": true,
    "outDir": "dist",
    "allowJs": true,
    "target": "es2020",
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowUmdGlobalAccess": true,
    "moduleResolution": "node",
    "module": "None",
    "outFile": "./out/melvorTypes",
    "noUnusedLocals": false,
    "strictNullChecks": false,
  },
  "include": ["./src/customTypes/*","./src/game/*","./src/gameTypes/*"],
  "exclude": ["./src/propLibs/**/*","interface/**/*"],
  "typeAcquisition": {
    "enable": true
  },
}